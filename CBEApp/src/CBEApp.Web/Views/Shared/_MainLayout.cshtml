@using System.Web.Optimization
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title</title>

    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/main.css">
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>

    @*<script src="~/Scripts/sidebarcontrol.js"></script>
        <link rel="stylesheet" href="~/Content/fab.css">*@

    <script type="text/javascript" src="~/Scripts/sidebarcontrol.js">

    </script>
</head>
<body>
    <div class="container" id="main">
        <!--Navigation Bar-->
        <div class="header">
            <nav class="navbar navbar-expand-md navbar-dark bg-dark sticky-top w-100 nav-fill">
                <a href="#" class="navbar-brand"><img src="~/images/JMFE-logo.png" width="100" /></a>
                <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav">
                        <a href="#" class="nav-item nav-link active">Registration</a>
                        <a href="#" class="nav-item nav-link">Check In</a>
                        <a href="#" class="nav-item nav-link">Tabulation</a>
                        <a href="#" class="nav-item nav-link">EMCEE</a>
                        <a href="#" class="nav-item nav-link">Cashier</a>
                        <a href="#" class="nav-item nav-link">Reports</a>
                        <a href="#" class="nav-item nav-link">Admin</a>
                    </div>
                    <div class="navbar-nav ml-auto">
                        <a href="#" class="nav-item nav-link">Login</a>
                    </div>
                </div>
            </nav>
        </div>
        <!--Navigation Bar end-->
        <!--Sidebar-->
        @{
            Html.RenderPartial("_PartialSideMenu");
        }
        <!--Sidebar end-->
        <!--Content. Present here for testing only. Will be a separate html injected through application logic-->

        <div class="container">

            @RenderBody()

        </div>
    </div>
</body>
</html>
